# Booking Intelligence System - Environment Configuration Template
# Copy this file to .env and fill in your actual credentials

# Supabase Configuration (REQUIRED for database migration)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here
SLACK_CHANNEL_ID=your_slack_channel_id_here
SLACK_SIGNING_SECRET=your_slack_signing_secret_here

# Email Configuration (Resend)
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=your_verified_sender_email_here

# Tavus Configuration (Optional - for video Q&A)
TAVUS_API_KEY=your_tavus_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development
LOG_LEVEL=info
SERVICE_NAME=booking-intelligence-system
