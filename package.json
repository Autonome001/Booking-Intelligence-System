{
  "name": "autonome-booking-agent",
  "version": "1.0.2-booking-fix-deploy",
  "type": "module",
  "description": "RAG-powered booking agent for Autonome strategy calls",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node backend/server.js",
    "setup-db": "node scripts/setup-database.js",
    "populate-faqs": "node scripts/populate-faqs.js",
    "test": "jest",
    "test:slack": "playwright test --config=playwright.config.js tests/slack-button-interaction.test.js",
    "test:slack:local": "playwright test --project=local-api-tests tests/slack-button-interaction.test.js",
    "test:slack:production": "playwright test --project=production-integration tests/slack-button-interaction.test.js",
    "test:slack:performance": "playwright test --project=performance-tests tests/slack-button-interaction.test.js",
    "test:slack:report": "playwright show-report playwright-report",
    "test:all": "npm run test && npm run test:slack",
    "build": "echo 'Backend dependencies handled by Dockerfile'"
  },
  "dependencies": {
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.12.0",
    "@supabase/supabase-js": "^2.56.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^142.0.0",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.0",
    "resend": "^3.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "booking",
    "rag",
    "ai",
    "automation",
    "slack",
    "supabase"
  ],
  "author": "Autonome",
  "license": "MIT"
}
